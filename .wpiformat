cppHeaderFileInclude {
  \.h$
  \.hpp$
  \.inc$
}

cppSrcFileInclude {
  \.cpp$
}

# Extra "/" used by unit tests
generatedFileExclude {
  /cpplint\.py$
}

modifiableFileExclude {
  \.png$
}

licenseUpdateExclude {
  Excluded\.h$
}

# Ordered this way to ensure tests find longest match
includeGuardRoots {
  wpiformat/
  wpiformat/test/
}
